@{
  Layout = "_Layout";
}

@using ProjectName.Models;

<h1>ParentObjects</h1>

@if (@Model.Count == 0)
{
  <h3>No ParentObjects have been added yet!</h3>
}

@foreach (ParentObject parentObject in Model)
{
  <li>@Html.ActionLink($"{parentObject.SomeProperty}", "Details", new { id = parentObject.ParentObjectId})</li>
}

<p>@Html.ActionLink("Add new parentObject", "Create")</p>